{
  "name": "redux-simple-starter",
  "version": "1.0.0",
  "description":
    "Simple starter package for Redux with React and Babel support",
  "main": "index.js",
  "repository": "git@github.com:StephenGrider/ReduxSimpleStarter.git",
  "scripts": {
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js",
    "prod": "NODE_ENV=production node server.js",
    "postinstall": "webpack -p",
    "test":
      "mocha --compilers js:babel-core/register --require jsdom-global --require mock-local-storage --require ignore-styles ./test/test_helper.js --recursive ./test",
    "coverage": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "test:watch": "npm run test -- --watch"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "babel-preset-react": "^6.1.18",
    "babel-register": "^6.26.0",
    "chai": "^3.5.0",
    "chai-jest-snapshot": "^2.0.0",
    "chai-jquery": "^2.0.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "ignore-styles": "^5.0.1",
    "jquery": "^2.2.1",
    "jsdom": "^8.5.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^2.5.3",
    "mock-local-storage": "^1.0.5",
    "moxios": "^0.4.0",
    "nyc": "^11.4.1",
    "prettier-eslint": "^8.6.0",
    "react-addons-test-utils": "^0.14.7",
    "react-test-renderer": "^16.2.0",
    "redux-mock-store": "^1.4.0",
    "style-loader": "^0.19.1",
    "webpack": "^1.12.9",
    "webpack-dev-server": "^1.14.0"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-preset-stage-1": "^6.1.18",
    "coveralls": "^3.0.0",
    "lodash": "^3.10.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "4.3.0",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "react-spinkit": "^3.0.0",
    "redux": "^3.0.4",
    "redux-axios-middleware": "^4.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-form": "^6.6.3",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "toastr": "^2.1.4"
  }
}
